<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.number_system_app">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Number converter"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Octal_conversion"></activity>
        <activity android:name=".Hexa_conversion" />
        <activity android:name=".Decimal_Conversion" />
        <activity android:name=".Binary_conversion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>